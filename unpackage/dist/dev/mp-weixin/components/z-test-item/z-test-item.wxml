<view><view class="text-item"><view class="item-up"><view class="title"><text>{{item.title}}</text><text>{{"时长："+item.expire+"分钟"}}</text></view><view class="info"><text>{{"题目总数："+item.question_count}}</text><text>{{"总分数："+item.total_score}}</text><text>{{"及格分数："+item.pass_score}}</text></view></view><view class="item-down"><view data-event-opts="{{[['tap',[['goToexam',['$0'],['item']]]]]}}" class="{{['btn',(item.is_test)?'active':'']}}" bindtap="__e">{{''+(item.is_test?'考试完毕':'参加考试')+''}}</view></view></view></view>